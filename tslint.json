{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "allow-empty-catch": true,
    "allow-snake-case": true,
    "no-shadowed-variable": true,
    "radix": true,
    "no-var-keyword": false,
    "indent": [ true, "tabs", 4 ],
    "only-arrow-functions": false,
    "no-console": false,
    "comment-format": false,
    "one-line": false,
    "eofline": false,
    "max-line-length": false,
    "no-bitwise": false
  },
  "rulesDirectory": []
}
